  "bracketSpacing": false,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "es5",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<BUILT_IN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(@shared)(/lib/.*)$",
    "",
    "^(@shared)(/types/.*)$",
    "",
    "^(@shared)(/icons/.*)$",
    "",
    "^(@shared)(/.*)$",
    "",
    "^(@src)(/components/.*)$",
    "",
    "^(@src)(/lib/.*)$",
    "",
    "^(@src)(/stores/.*)$",
    "",
    "^(@src)(/types/.*)$",
    "",
    "^(@src)(/resources/.*)$",
    "",
    "^(@src)(/.*)$",
    "",
    "^[.]"
  ],
  "overrides": [
    {
      "files": "*.svg",
      "options": {
        "parser": "html"
      }
    }
  ]
}
